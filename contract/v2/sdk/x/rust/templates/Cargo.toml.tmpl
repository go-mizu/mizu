{{define "Cargo.toml.tmpl"}}# Code generated by sdkrust. DO NOT EDIT.

[package]
name = {{rustString .Crate}}
version = {{rustString .Version}}
edition = {{rustString .Edition}}
{{- if .Authors}}
authors = [{{range $i, $a := .Authors}}{{if $i}}, {{end}}{{rustString $a}}{{end}}]
{{- end}}
{{- if .Service.Description}}
description = {{rustString .Service.Description}}
{{- end}}
{{- if .Repository}}
repository = {{rustString .Repository}}
{{- end}}
{{- if .Documentation}}
documentation = {{rustString .Documentation}}
{{- end}}
license = "MIT OR Apache-2.0"
keywords = ["api", "sdk", {{rustString .Crate}}]
categories = ["api-bindings", "web-programming::http-client"]

[features]
default = ["native-tls"]
native-tls = ["reqwest/native-tls"]
rustls-tls = ["reqwest/rustls-tls"]
{{- if .HasDate}}
chrono = ["dep:chrono"]
{{- end}}

[dependencies]
bytes = "1"
futures-core = "0.3"
reqwest = { version = "0.12", default-features = false, features = ["json", "stream"] }
serde = { version = "1", features = ["derive"] }
serde_json = "1"
thiserror = "2"
tokio = { version = "1", features = ["time"] }
pin-project-lite = "0.2"
{{- if .HasDate}}
chrono = { version = "0.4", features = ["serde"], optional = true }
{{- end}}

[dev-dependencies]
tokio = { version = "1", features = ["full", "macros"] }
{{end}}
