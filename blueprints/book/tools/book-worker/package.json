{
  "name": "book-worker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx wrangler dev",
    "deploy": "npx wrangler deploy",
    "typecheck": "npx tsc --noEmit",
    "test": "npx vitest run",
    "db:init": "npx wrangler d1 execute book-db --file=src/schema.sql",
    "db:init:local": "npx wrangler d1 execute book-db --local --file=src/schema.sql"
  },
  "dependencies": {
    "hono": "^4.11.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260210.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.63.0"
  }
}
